{
  "name": "opentelemetry-amqp-baggage-propagation",
  "version": "1.0.0",
  "description": "OpenTelemetry AMQP baggage propagation example with RabbitMQ",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "publisher": "npx tsx --experimental-loader=@opentelemetry/instrumentation/hook.mjs --import @opentelemetry/auto-instrumentations-node/register src/publisher.ts",
    "consumer": "npx tsx --experimental-loader=@opentelemetry/instrumentation/hook.mjs --import @opentelemetry/auto-instrumentations-node/register src/consumer.ts",
    "api-service": "npx tsx --experimental-loader=@opentelemetry/instrumentation/hook.mjs --import @opentelemetry/auto-instrumentations-node/register src/api-service.ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.64.6",
    "amqplib": "^0.10.4",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.5",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.19.0"
  }
}